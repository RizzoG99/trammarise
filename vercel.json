{
  "devCommand": "npm run dev",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ],
  "functions": {
    "api/transcribe.ts": {
      "maxDuration": 300,
      "memory": 1024,
      "includeFiles": "node_modules/{@ffmpeg-installer,@ffprobe-installer}/**"
    },
    "api/**/*.ts": {
      "maxDuration": 60
    }
  }
}
